{
  "name": "chimee-kernel-hls",
  "version": "1.3.2",
  "description": "kernel-hls of chimee",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "jsnext:main": "lib/index.mjs",
  "browser": "lib/index.browser.js",
  "scripts": {
    "precommit": "npm run lint",
    "prepush": "npm run test",
    "prepublishOnly": "npm run lint && npm t && pkg-ok",
    "build": "npm run b-common && npm run b-es && npm run b-umd && npm run b-min",
    "b-common": "rollup -c build/rollup.config.common.js",
    "b-es": "rollup -c build/rollup.config.es.js",
    "b-umd": "rollup -c build/rollup.config.umd.js",
    "b-min": "rollup -c build/rollup.config.min.js",
    "start": "rollup -c build/rollup.config.dev.js -w",
    "lint": "eslint . --fix && flow check",
    "flow": "flow check",
    "test": "karma start karma.conf.js",
    "unit": "karma start karma.conf.js --auto-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chimeejs/chimee-kernel-hls.git"
  },
  "keywords": [
    "kernel",
    "chimee",
    "video",
    "hls"
  ],
  "author": "songguangyu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Chimeejs/chimee-kernel-hls/issues"
  },
  "homepage": "https://github.com/Chimeejs/chimee-kernel-hls#readme",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "chimee-helper": "^0.2.11",
    "hls.js": "^0.11.0",
    "toxic-decorators": "^0.3.8"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "chimee-kernel": "^1.4.0",
    "eslint": "^5.8.0",
    "eslint-config-egg": "^7.0.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "flow-bin": "^0.85.0",
    "husky": "^1.1.3",
    "karma": "^3.1.1",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-coveralls": "^2.1.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-rollup-preprocessor": "^6.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "mocha": "^5.2.0",
    "pkg-ok": "^2.2.0",
    "puppeteer": "^1.4.0",
    "rollup": "^0.67.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-flow-no-whitespace": "^1.0.0",
    "rollup-plugin-includepaths": "^0.2.2",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.6.0",
    "rollup-plugin-uglify": "^6.0.0",
    "rollup-watch": "^4.3.1"
  }
}
